<%= form_tag 'personnel_create' do %>

 <h1>Generate Personnel</h1>
	<label>Name:</label><%= text_field_tag "name"," " %><br>
	<br>
	<label>Email:</label><%= text_field_tag "email"," " %><br>
	<br>
	<label>Password:</label><%= text_field_tag "password", " " %><br>
	<br>
	<label>Password Confirmation:</label><%= text_field_tag "password_digest", " " %><br>
	<br>
	<%= submit_tag 'Create Personnel' %>
<%end%>	
